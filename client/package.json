{
    "name": "@design-tool/client",
    "version": "0.0.1",
    "main": "dist/index.js",
    "author": "Johan Sj√∂berg",
    "license": "MIT",
    "private": true,
    "scripts": {
        "prettier": "prettier --write 'src/**/*.{ts,tsx,mdx}' --ignore-path .gitignore",
        "clean": "rm -rf dist",
        "test": "jest",
        "test:watch": "jest --watch",
        "lint": "eslint --ignore-path ./.gitignore --fix",
        "build:dev": "cp -v .env.dev .env && snowpack build",
        "build:stag": "cp -v .env.staging .env && snowpack build",
        "build:prod": "cp -v .env.prod .env && snowpack build",
        "start:dev": "cp -v .env.dev .env && snowpack dev",
        "start:staging": "cp -v .env.staging .env && snowpack dev",
        "start:prod": "cp -v .env.prod .env && snowpack dev",
        "start:fetchtypes": "yarn graphql:types && snowpack dev",
        "graphql:types": "graphql-codegen --config codegen.yml"
    },
    "dependencies": {
        "@apollo/client": "^3.3.7",
        "@apollo/react-hooks": "^4.0.0",
        "@reach/router": "^1.3.4",
        "axios": "^0.21.1",
        "graphql": "^15.5.0",
        "graphql-tag": "^2.11.0",
        "jwt-decode": "^3.1.2",
        "node-sass": "^5.0.0",
        "react": "^17.0.1",
        "react-redux": "^7.2.2",
        "react-rnd": "^10.2.4",
        "react-router-dom": "^5.2.0",
        "redux": "^4.0.5",
        "redux-devtools-extension": "^2.13.8",
        "redux-thunk": "^2.3.0",
        "typescript": "^4.1.3"
    },
    "devDependencies": {
        "@graphql-codegen/cli": "1.20.1",
        "@graphql-codegen/typescript": "^1.20.2",
        "@graphql-codegen/typescript-operations": "1.17.14",
        "@graphql-codegen/typescript-react-apollo": "2.2.1",
        "@habx/apollo-multi-endpoint-link": "^2.4.0",
        "@rollup/plugin-node-resolve": "^11.1.1",
        "@snowpack/plugin-dotenv": "^2.0.5",
        "@snowpack/plugin-react-refresh": "^2.4.0",
        "@snowpack/plugin-sass": "^1.3.0",
        "@types/http-proxy": "^1.17.5",
        "@types/ramda": "^0.27.36",
        "@types/reach__router": "^1.3.7",
        "@types/react": "^17.0.0",
        "@types/react-dom": "^17.0.0",
        "@types/react-redux": "^7.1.16",
        "@types/react-rnd": "^8.0.0",
        "@types/react-router-dom": "^5.1.7",
        "@types/redux": "^3.6.0",
        "@types/redux-thunk": "^2.1.0",
        "@typescript-eslint/eslint-plugin": "^4.4.0",
        "@typescript-eslint/parser": "^4.4.0",
        "eslint": "^7.18.0",
        "http-proxy": "^1.18.1",
        "prettier": "^2.2.1",
        "ramda": "^0.27.1",
        "react-dom": "^17.0.1",
        "rollup": "^2.38.1",
        "rollup-plugin-livereload": "^2.0.0",
        "rollup-plugin-postcss": "^3.1.8",
        "rollup-plugin-serve": "^1.1.0",
        "rollup-plugin-terser": "^7.0.2",
        "rollup-plugin-typescript2": "^0.29.0",
        "snowpack": "^3.0.11"
    }
}
